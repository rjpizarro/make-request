{
  "name": "@yuniku/make-request-project",
  "version": "1.0.0",
  "description": "A package to build an API caller for Web and Mobile",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --watch",
    "build": "webpack -p --display-error-details",
    "link-web": "cd src/make-request-web && npm link",
    "version-web": "cd src/make-request-web && npm version",
    "version-mobile": "cd src/make-request-mobile && npm version",
    "deploy-web": "cd src/make-request-web && npm publish",
    "deploy-mobile": "cd src/make-request-mobile && npm publish",
    "deploy": "npm run deploy-web && cd .. && npm run deploy-mobile",
    "visualize-bundle": "webpack --profile --json > webpack-stats.json",
    "analyze": "webpack -p --profile"
  },
  "keywords": [
    "api",
    "request",
    "yuniku",
    "redux",
    "axios",
    "post",
    "get",
    "put",
    "delete"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/yuniku/packages/make-request.git"
  },
  "dependencies": {
    "@material-ui/core": "1.5.1",
    "arraybuffer-to-string": "1.0.2",
    "axios": "0.17.1",
    "firebase": "^5.5.4",
    "lodash": "4.17.13",
    "normalizr": "3.2.4",
    "prop-types": "15.6.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-spinkit": "3.0.0",
    "redux": "3.7.2",
    "reselect": "3.0.1",
    "webpack": "3.10.0"
  },
  "devDependencies": {
    "@material-ui/icons": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-env": "1.5.1",
    "css-loader": "0.28.9",
    "dot-prop-immutable": "1.4.0",
    "postcss-loader": "2.0.10",
    "react-scroll-to-component": "1.0.2",
    "react-transition-group": "^2.5.0",
    "recompose": "0.30.0",
    "style-loader": "0.19.1"
  },
  "author": "Yuniku Inc.",
  "license": "ISC"
}
